			<script src="{{asset ('assets1/js/popper.min.js')}}"></script>
			<script src="{{asset ('assets1/js/bootstrap.min.js')}}"></script>

			<!-- Slimscroll JS -->
			<script src="{{asset ('assets1/plugins/slimscroll/jquery.slimscroll.min.js')}}"></script>

			<script src="{{asset ('assets1/plugins/raphael/raphael.min.js')}}"></script>
			<script src="{{asset ('assets1/plugins/morris/morris.min.js')}}"></script>
			<script src="{{asset ('assets1/js/chart.morris.js')}}"></script>

			<!-- Custom JS -->
			<script src="{{asset ('assets1/js/script.js')}}"></script>
		{% endblock %}

	</body>
</html>
